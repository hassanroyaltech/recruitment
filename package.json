  "scripts": {
    "start": "(npm run lint:fix & react-scripts --max_old_space_size=8000 start)",
    "build": "(npm run lint:fix & react-scripts --max_old_space_size=8000 build)",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src",
    "tsc": "tsc",
    "test-c": "npm run prettier-fix && npm run lint:fix && tsc",
    "test-all": "npm run prettier-fix && npm run lint:fix && tsc",
    "prettier-fix": "prettier \"**/*\" --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "react",
      "import",
      "react-hooks",
      "jsx-a11y",
      "testing-library"
    ],
    "env": {
      "browser": true,
      "es2020": true,
      "amd": true,
      "node": true,
      "jasmine": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "settings": {
      "format_on_save": true,
      "import/resolver": {
        "node": {
          "paths": [
            "src",
            "node_modules"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "rules": {
      "react/prop-types": 1,
      "import/no-duplicates": [
        "error",
        {
          "considerQueryString": true
        }
      ],
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "curly": [
        "error",
        "multi"
      ],
      "import/no-cycle": [
        2,
        {
          "maxDepth": 1,
          "ignoreExternal": true
        }
      ],
      "camelcase": "off",
      "react/jsx-indent": "off",
      "no-unused-vars": "warn",
      "operator-linebreak": [
        "error",
        "before"
      ],
      "react/no-direct-mutation-state": "warn",
      "indent": [
        "error",
        2
      ],
      "no-duplicate-imports": "error"
    },
    "globals": {
      "Vitally": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
